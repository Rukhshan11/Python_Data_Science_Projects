content_ratings = {'4+': 4433, '12+': 1155, '9+': 987, '17+': 622}
total_number_of_apps = 7197
c_ratings_proportions = {}
c_ratings_percentages ={}

for key in content_ratings:
    proportions = content_ratings[key] / total_number_of_apps
    percentage = proportions * 100
    
    c_ratings_percentages[key] = percentage
    c_ratings_proportions[key] = proportions
    
    print('key:', key)
    print(' proportion value:', proportions)
    print(' percentage value:', percentage)
